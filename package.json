{
  "name": "chronos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "npm run compile && vite build && serve -s stats.html",
    "start": "vite",
    "compile": "tsc && lingui compile",
    "build:dev": "npm run compile && vite build",
    "build:prod": "npm run compile && vite build --mode prod",
    "build:staging": "npm run compile && vite build --mode staging",
    "build:studio": "npm run compile && vite build --mode studio",
    "build:uat": "npm run compile && vite build --mode uat",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview",
    "trans:extract": "lingui extract",
    "trans:compile": "lingui compile"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.1",
    "@json2csv/plainjs": "^7.0.6",
    "@lingui/macro": "^4.7.1",
    "@lingui/react": "^4.7.1",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tremor/react": "^3.13.3",
    "dayjs": "^1.11.10",
    "js-base64": "^3.7.6",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "sort-by": "^0.0.2",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.1",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@lingui/cli": "^4.7.1",
    "@lingui/vite-plugin": "^4.7.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jquery": "^3.5.29",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-compression2": "^0.12.0"
  }
